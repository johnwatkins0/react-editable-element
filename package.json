{
  "name": "react-editable-html-element",
  "version": "1.0.0",
  "description": "Allows HTML elements to be modified in place by changing them into inputs when clicked.",
  "main": "src/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwatkins0/react-editable-html-element.git"
  },
  "keywords": [
    "react",
    "javascript"
  ],
  "author": "John Watkins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johnwatkins0/react-editable-html-element/issues"
  },
  "homepage": "https://github.com/johnwatkins0/react-editable-html-element#readme",
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb",
      "google"
    ],
    "globals": {
      "import": true,
      "wpData": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "error"
          ]
        }
      ],
      "react/jsx-filename-extension": 0,
      "require-jsdoc": 0
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-google": "^0.8.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1"
  }
}
